{
  "name": "simple-desktop-images",
  "version": "1.0.0",
  "description": "Image scraper for simple desktops",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/barrythepenguin/simple-desktop-images.git"
  },
  "author": "Jonathan Haines",
  "scripts": {
    "build": "del-cli dist && tsc",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "start": "node dist/src/index.js",
    "test": "xo && tsc --noEmit && nyc ava",
    "test:watch": "ava --watch"
  },
  "dependencies": {
    "@reactivex/rxjs": "^6.5.5",
    "cheerio": "^1.0.0-rc.3",
    "got": "^11.1.1",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.18",
    "@types/node": "^13.13.4",
    "ava": "^3.8.2",
    "codecov": "^3.6.5",
    "del-cli": "^3.0.0",
    "nock": "^12.0.3",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "prettier-plugin-package": "^1.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3",
    "xo": "^0.30.0"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "test/": "dist/test/"
      }
    }
  },
  "xo": {
    "prettier": true,
    "envs": [
      "node"
    ],
    "rules": {
      "@typescript-eslint/prefer-readonly-parameter-types": "off"
    }
  }
}

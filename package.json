{
  "name": "simple-desktop-images",
  "version": "1.0.0",
  "description": "Image scraper for simple desktops",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/barrythepenguin/simple-desktop-images.git"
  },
  "author": "Jonathan Haines",
  "scripts": {
    "build": "del-cli dist && tsc",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "start": "node dist/src/index.js",
    "test": "xo && tsc --noEmit && c8 ava",
    "test:watch": "ava --watch"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "got": "^12.0.4"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/node": "^17.0.34",
    "ava": "^4.3.0",
    "c8": "^7.11.3",
    "codecov": "^3.8.3",
    "del-cli": "^4.0.1",
    "nock": "^13.2.6",
    "prettier": "^2.6.2",
    "prettier-plugin-package": "^1.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.3",
    "xo": "^0.48.0"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "test/": "dist/test/"
      },
      "compile": false
    }
  },
  "xo": {
    "prettier": true,
    "envs": [
      "node"
    ]
  }
}

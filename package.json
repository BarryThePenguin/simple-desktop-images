{
  "name": "simple-desktop-images",
  "version": "1.0.0",
  "description": "Image scraper for simple desktops",
  "scripts": {
    "start": "babel-node src/index.js",
    "flow": "flow",
    "test": "xo && flow && nyc --reporter lcov --reporter text-lcov ava",
    "test:watch": "ava --watch",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "author": "Jonathan Haines",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.26.0",
    "codecov": "^2.3.0",
    "flow-bin": "^0.54.1",
    "moxios": "^0.4.0",
    "nyc": "^11.1.0",
    "xo": "^0.18.2"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "cheerio": "^1.0.0-rc.2",
    "rxjs": "^5.4.3"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  },
  "xo": {
    "esnext": true,
    "parser": "babel-eslint",
    "envs": [
      "node"
    ],
    "ignores": [
      "flow-typed/**"
    ]
  }
}
